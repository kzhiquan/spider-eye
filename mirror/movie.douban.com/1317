<script>
    $(function(){
        $(".list-comm").delegate(".show_full_str","click",function(){
            var $this = $(this);
            $this.prevAll(".short_str, .elp").hide();
            $this.prevAll(".full_str").show();
            $this.text("收起").attr("class","hide_full_str");
            return false;
        }).delegate(".hide_full_str","click",function(){
            var $this = $(this);
            $this.prevAll(".short_str, .elp").show();
            $this.prevAll(".full_str").hide();
            $this.text("展开").attr("class","show_full_str");
            return false;
        })
    })
</script>
<script id="comm-mod-tmpl" type="text/x-jquery-tmpl">
    <div class="comm-mod" data-cid={{= cid}}> 
        <div class="hd">
            <a href="{{= user_url}}/"><img src="{{= user_ava}}" /></a>
        </div>
        <div class="bd">
            <h4>
                <a href="{{= user_url}}">{{= user_name}}</a> 
                <span>{{= pub_time}}</span>
                {{if can_delete}}
                <a href="javascript:void(0)" data-cid="{{= cid}}" data-type="reply" class="del gact" title="删除">X</a>
                {{/if}}
            </h4>
            <p>
                {{if target}}
                [第{{= target}}集]
                {{/if}}
                {{if reply_cont.replace(/[^\x00-\xff]/g,"**").length > 300}}
                    <span class='short_str'>{{= reply_cont.substring(0,300)}} </span>
                    <span class='full_str' style='display:none'> {{= reply_cont}} </span>
                    <span class='elp'>...</span><a href='#' class='show_full_str'>展开</a>
                {{else}}
                    {{= reply_cont}}
                {{/if}}
                {{if replys}}
                <a href="javascript:void(0)" data-hasrep="0" class="replys">0回应</a>
                {{/if}}
            </p>
        </div>
    </div> 
</script>




    
    

    
