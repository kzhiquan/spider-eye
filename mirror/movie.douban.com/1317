<h2>讨论区 &nbsp; ·&nbsp; ·&nbsp; ·&nbsp; ·&nbsp; ·&nbsp; ·</h2>



<script>
Do.add('tmpl', {path: 'http://img3.douban.com/js/lib/packed_jquery.tmpl.min9524192953.js', type: 'js'});
Do.add('dialog-css', {path: 'http://img3.douban.com/css/ui/packed_dialog4563741467.css', type: 'css'});
Do.add('dialog', {path: 'http://img3.douban.com/js/ui/packed_dialog8321689615.js', type: 'js', requires: ['dialog-css']});
Do.ready("tmpl","dialog", function(){
    $(".liveLogin").live("click",function(){
        $(".a_show_login").eq(0).click();
    });
    //提交评论
    $("#comment-section").delegate("#new-comm,.new-reply","submit",function(){
        var frm = $(this),
            serdata = frm.serialize(),
            arrdata = frm.serializeArray(),
            uri = frm.attr("action"),
            subt = frm.find(":submit"),
            owner = frm.attr("id"),
            sel = frm.find("select");
            
        var comment_txt = $.trim(frm.find("textarea[name=discussion]").val()); 
        var reply_txt = $.trim(frm.find("input[name=reply]").val()); 
        if (!comment_txt && !reply_txt){
            return false;
        } else {
            subt.val("发送中").attr("disabled","disabled");
            $.ajax({
                type: "post",
                data: serdata,
                url: uri,
                success : function(e){
                   if (e.result==="success"){ 
                       var comm_data = {
                           cid : e.cid,			   
                       };
                       if (owner === "new-comm"){
                          //添加新的评论
                           comm_data.reply_cont = comment_txt;
                           comm_data.replys = true;
						   if(sel){
							   if (sel.val()!=="0"){
								   comm_data.target = sel.val();
                               }
						   };
                           $("#comm-mod-tmpl").tmpl(comm_data).prependTo($(".add-comm").next(".list-comm"));
                           frm.find("textarea[name=discussion]").val(""); 
                           frm.find(".enable").removeClass("enable"); 
                           subt.val("我说").removeAttr("disabled");
                           $("#comm_show").show();
                           $(".add-comm").hide();
                       }else{
                          //添加新的回复
                          comm_data.reply_cont = reply_txt;
                          comm_data.replys = false; 
                          comm_data.cid = e.reply_id; 
                          $("#comm-mod-tmpl").tmpl(comm_data).insertBefore(frm.closest(".comm-mod"));
                          frm.find("input[name=reply]").val(""); 
                          subt.val("发表回应").removeAttr("disabled");
                       };
                   } else if(e.result.if_refreshed) {
                       var dlg = dui.Dialog({
                           title: '提示',
                           content: e.reason, 
                           buttons: [{text: '知道了', method: function(o){ o.close(); }}],
                           width: 400
                       }, true);
                       dlg.open().update();
                       subt.val("我说").removeAttr("disabled");
                   } else {
                       var dlg = dui.Dialog({
                           title: '提示',
                           content: e.reason, 
                           buttons: [{text: '知道了', method: function(o){ o.close(); }}],
                           width: 400
                       }, true);
                       dlg.open().update();
                       frm.find("input[name=reply]").val(""); 
                       subt.val("我说").removeAttr("disabled");
                   };
                   return false;
                },
                error: function(){
                    alert("发送失败,请重试.");
                    if (owner === "new-comm"){
                        subt.val("发表回应").removeAttr("disabled");
                    }else{
                        subt.val("我说").removeAttr("disabled");
                    }
                    return false;
                }

            });
            return false;
        };
    });

    $("#comment-section").delegate(".del", "click", function(){
        var $this = $(this),
            cid = $this.data("cid"),
            mod = $this.parents(".comm-mod"),
            type = $this.data("type");

        if(type === "reply"){
            var url = "/j/tv/discussion/remove_reply";
            var data = {"reply_id": cid, "ck": "None"};
        } else if(type === "discussion") {
            var url = "/j/tv/discussion/remove_discussion";
            var data = {"discussion_id": cid, "ck": "None"};
        }
        
        var uri = $this.attr("href");
        if (confirm('您确定删除吗?')){
            $.ajax({
                url: url,
                data: data,
                type: "post",
                success: function(e){
                    if(e.result==="success"){
                        mod.eq(0).remove();
                    }
                }
            })
        }
        return false;
    });

    //获取&显示添加回应
    $("#comment-section").delegate(".replys", "click", function(){
        var link = $(this),
            fa_node = link.parents(".comm-mod"),
            son_list = fa_node.find(".list-comm"), 
            rep_num = link.data("hasRep"), 
            cid = fa_node.data("cid"),
            data = {more:rep_num, cid:cid};
        if(!son_list.length){
            $("#rep-tmpl").tmpl(data).appendTo(fa_node);
            fa_node.find(".list-comm").find(".cid").attr("value",cid);
            var reply_box = fa_node.find(".reply-box");
            get_more_rep(reply_box, cid, 0, rep_num);
        }else{
            son_list.remove();
            link.text(rep_num + "回应");
        };
        return false;
    });

    //更多评论
    var c = 0;
    $(".list-comm").delegate(".load-more a", "click", function(){
        var cid = $(this).parents(".comm-mod").last().data("cid");
        var reply_box = $(this).parents(".reply-box");
        get_more_rep(reply_box, cid, c+=30, 30);
        return false;
    });

    //获得评论
    function get_more_rep(fa_node, cid, start, count){
        var start = start || 0,
            count = count >= 30 ? 30 : count,
            link = fa_node.parents(".comm-mod").find(".replys");

        link.text("加载中...");
        $.get("/j/tv/discussion/replies",{discussion_id: cid, start:start, count:count},function(data){
            $("#comm-mod-tmpl").tmpl(data).insertBefore(fa_node);
            var show_comm_len = fa_node.siblings(".comm-mod").length;
            if (link.data("hasrep") > show_comm_len){
                fa_node.find(".load-more").show();
            } else {
                fa_node.find(".load-more").hide();
            };
            link.text("隐藏回应");
        });
    };
    
    //获取焦点时显示按钮
    $("#new-comm .comment").focus(function(){
        var that = $(this);
        var submit = that.next(".comm-op").find(":submit"); 
        if($.trim($(that).val())){
            submit.addClass("enable");
        } else {
            submit.removeClass("enable");
        };
    }).keyup(function(){
        var that = $(this);
        var submit = that.next(".comm-op").find(":submit"); 
        if($.trim($(that).val())){
            submit.addClass("enable");
        } else {
            submit.removeClass("enable");
        };
    });

    //显示讨论框
    $("#comm_show").bind("click",function(){
        $(this).hide();
        $(".add-comm").show();
        return false;
    });

    //隐藏讨论框
    $("#new-comm").find("input[type=reset]").click(function(){
        $("#comm_show").show();
        $(".add-comm").hide();
    });
});
</script>

