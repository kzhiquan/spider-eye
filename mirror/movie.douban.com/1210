    <script type="text/javascript">
        //-----------------------
        //object for flash player
        //-----------------------
        var playerCB = {
            showShare: function(w){
                if (w==="stop"){
                    $(".playerBt").addClass("replay");
                } else if (w==="pause"){
                    $(".playerBt").removeClass("replay");
                }
                $("#player-cover,#player-cover-mask").show();
            },
            hideShare: function(){
                $("#player-cover,#player-cover-mask").hide();
            },
            ck: function(){
                return "None";
            }
        }

        $(function(){
            var list_num  = $(".video-list-col").length,
                list_wi = $(".video-list-col").css("width"),
                list_op = $("#video-list-op"),
                arrR = list_op.find(".r"),
                arrL = list_op.find(".l"),
                trailer_now = $("#player").attr("trailer_id"),
                list_now = parseInt( trailer_now / 3 ) + 1,
                player_cover = $("#player-cover"),
                hl;

            //set default row
            $("#video-list-wp").css("left", -(parseInt(list_wi)*(list_now-1))+"px");

            //set current heightlight trailer
            $("#v_" + trailer_now).addClass("sellect");

            // if display arrow button
            if (list_num > 1){
                list_op.css("visibility","visible");
            }

            //set arrow
            if (list_now === list_num){
                arrR.addClass("dis");
            }

            if (list_now === 1){
                arrL.addClass("dis");
            }

            //scrool right
            list_op.delegate(".r", "click", function(){
                if (list_now < list_num) {
                    list_now ++;
                    $("#video-list-wp").animate({left: "-=" + list_wi}, 300, function(){
                        arrL.removeClass("dis");
                        if(list_now >= list_num){
                            arrR.addClass("dis");
                        }
                    });
                }
            });

            //scrool left
            list_op.delegate(".l", "click", function(){
                if (list_now > 1) {
                    list_now --;
                    $("#video-list-wp").animate({left: "+=" + list_wi}, 300, function(){
                        arrR.removeClass("dis");
                        if(list_now <= 1){
                            arrL.addClass("dis");
                        }
                    });
                }
            });

            //-------------------
            //Player Cover
            //-------------------
            player_cover.delegate(".playerBt", "click", function(){
                $("#movie_player")[0].exec('play');
                playerCB.hideShare();
            });
            player_cover.delegate(".replay", "click", function(){
                $("#movie_player")[0].exec('replay');
                playerCB.hideShare();
            })
            player_cover.delegate(".shuo", "click", function(){
                $(".btns").find(".lnk-sharing").click();
                return false;
            })

        })

    </script>
